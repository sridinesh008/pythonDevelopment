import csv

with open('Book1.csv') as csvfile:
    readCSV = csv.reader(csvfile, delimiter=',')
    names=[]
    ages=[]
    for row in readCSV:
        #print(row)
        name=row[0]
        age=row[1]
        names.append(name)
        ages.append(age)
print(names,ages)
whatAge = input('What person do you wish to know the age of?:')
coldex = names.index(whatAge)
age = ages[coldex]
print('The date of',whatAge,'is:',age)
